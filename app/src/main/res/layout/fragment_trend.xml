<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_actionbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/transparent"
        android:paddingTop="50dp"
        android:paddingBottom="10dp"
        app:layout_constraintBottom_toTopOf="@id/layout_state"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:maxEms="8"
            android:lines="1"
            android:id="@+id/tv_trends_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginStart="20dp"
            android:contentDescription="logo"
            android:gravity="center_vertical"
            android:text="@string/title_trends"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/iv_generation_agp"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginEnd="20dp"
            android:contentDescription="user center"
            android:padding="5dp"
            android:src="@drawable/ic_generation_agp"
            app:layout_constraintBottom_toBottomOf="@id/tv_trends_title"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/tv_trends_title"
            tools:ignore="HardcodedText" />

        <LinearLayout
            android:id="@+id/rl_date_space"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintTop_toBottomOf="@id/tv_trends_title">


            <TextView
                android:id="@+id/time_begin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="5dp"
                android:textColor="?attr/appColorAccent"
                android:textSize="18sp"
                tools:text="2018/4/1" />

            <TextView
                android:id="@+id/time_separate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:padding="3dp"
                android:text="@string/time_divider"
                android:textColor="?attr/appColorAccent"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/time_end"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="5dp"
                android:textColor="?attr/appColorAccent"
                android:textSize="18sp"
                tools:ignore="RtlHardcoded"
                tools:text="2018/4/1" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="6dp"
                android:src="@drawable/ic_date_arrow"
                tools:ignore="ContentDescription" />

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/home_scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <!--    APP-SRC-A-7-6-->

        <LinearLayout
            android:id="@+id/ll_scroll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/shape_circle_light"
                android:paddingTop="20dp"
                android:paddingBottom="10dp">

                <androidx.constraintlayout.widget.Guideline
                    android:layout_width="1dp"
                    android:id="@+id/guide_line_start"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.1"/>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guide_line_end"
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.55"/>

                <TextView
                    android:id="@+id/tv_cover_time_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="@string/cgm_cover_time"
                    android:textSize="14sp"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toEndOf="@id/guide_line_start"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="@id/tv_cover_time_title"
                    app:layout_constraintTop_toBottomOf="@id/tv_cover_time_title">

                    <TextView
                        android:id="@+id/tv_cover_time_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="0"
                        android:textSize="35sp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:textColor="?trendSmallTextColor"
                        android:id="@+id/tv_cover_time_percent"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="3dp"
                        android:layout_marginBottom="8dp"
                        android:gravity="center"
                        android:text="%"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@id/tv_cover_time_value"
                        app:layout_constraintStart_toEndOf="@id/tv_cover_time_value" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/tv_monitor_times_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="@string/monitor_times"
                    android:textSize="14sp"
                    app:layout_constraintStart_toEndOf="@id/guide_line_end"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tv_monitor_times_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="0"
                    android:textSize="35sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@id/tv_monitor_times_title"
                    app:layout_constraintTop_toBottomOf="@id/tv_monitor_times_title" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <!--            APP-SRC-A-7-1-->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/shape_circle_light"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">

                <androidx.constraintlayout.widget.Guideline
                    android:layout_width="1dp"
                    android:id="@+id/guide_line_one"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.1"/>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guide_line_two"
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.55"/>

                <TextView
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="@id/guide_line_one"
                    android:id="@+id/txt_title_glucose"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawablePadding="10dp"
                    android:text="@string/trends_content2"
                    android:textSize="14sp"
                    app:drawableEndCompat="@drawable/ic_question" />

                <LinearLayout
                    android:id="@+id/ll_ehba1c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:orientation="vertical"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toStartOf="@id/txt_title_glucose"
                    app:layout_constraintTop_toBottomOf="@id/txt_title_glucose">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/trend_hemoglobin"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textSize="35sp"
                            android:textStyle="bold"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/iv_hemoglobin_trends"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="6dp"
                            android:src="@drawable/ic_trends_up"
                            app:layout_constraintBottom_toTopOf="@+id/txt_unit_percent"
                            app:layout_constraintLeft_toRightOf="@+id/trend_hemoglobin"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:textColor="?trendSmallTextColor"
                            android:id="@+id/txt_unit_percent"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="6dp"
                            android:layout_marginBottom="6dp"
                            android:text="%"
                            app:layout_constraintBottom_toBottomOf="@+id/trend_hemoglobin"
                            app:layout_constraintLeft_toRightOf="@+id/trend_hemoglobin" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>

                <TextView
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/guide_line_two"
                    android:id="@+id/tv_average"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableEnd="@drawable/ic_question"
                    android:drawablePadding="10dp"
                    android:gravity="center"
                    android:text="@string/trends_content3"
                    android:textSize="14sp" />

                <LinearLayout
                    android:id="@+id/ll_mbg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:orientation="vertical"
                    app:layout_constraintStart_toStartOf="@id/tv_average"
                    app:layout_constraintTop_toBottomOf="@id/tv_average">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/tv_glucose_average"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textSize="35sp"
                            android:textStyle="bold"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/iv_glucose_trends"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="6dp"
                            android:src="@drawable/ic_trends_up"
                            app:layout_constraintBottom_toTopOf="@+id/tv_unit"
                            app:layout_constraintLeft_toRightOf="@+id/tv_glucose_average" />

                        <TextView
                            android:textColor="?trendSmallTextColor"
                            android:text=""
                            android:id="@+id/tv_unit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="6dp"
                            android:layout_marginBottom="6dp"
                            app:layout_constraintBottom_toBottomOf="@+id/tv_glucose_average"
                            app:layout_constraintLeft_toRightOf="@+id/tv_glucose_average" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/txt_title_tir"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:drawableEnd="@drawable/ic_question"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:text="@string/low_hi_glucose"
                android:textSize="16sp"
                android:textStyle="bold" />

            <!--APP-SRC-A-7-2-->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="10dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:background="@drawable/shape_circle_light">

                <RelativeLayout
                    android:id="@+id/rl_pie_chart"
                    android:layout_width="200dp"
                    android:layout_height="175dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/ll_pie_chart_detail"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.microtechmd.cgms.fragment.trends.PieChartView
                        android:id="@+id/pie_chart"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:circleRadius="50dp"
                        app:textSize="12sp" />

                    <TextView
                        android:id="@+id/txt_empty_pies"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/shape_trends_pies"
                        android:gravity="center"
                        android:text="@string/empty_data" />
                </RelativeLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ll_pie_chart_detail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/rl_pie_chart"
                    app:layout_constraintTop_toTopOf="parent">

                    <LinearLayout
                        app:layout_constraintVertical_chainStyle="packed"
                        app:layout_constraintBottom_toTopOf="@id/desc_high"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:id="@+id/ll_high"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical">

                        <ImageView
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/shape_trend_c2"
                            tools:ignore="ContentDescription" />

                        <TextView
                            style="@style/trend_text_TIR"
                            android:text="@string/hyper" />

                        <TextView
                            android:id="@+id/high_percent"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:drawablePadding="6dp"
                            android:text="0%" />

                        <ImageView
                            android:id="@+id/iv_high_trends"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_marginStart="6dp"
                            android:src="@drawable/ic_trends_up"
                            android:visibility="visible"
                            app:layout_constraintBottom_toTopOf="@+id/tv_unit"
                            app:layout_constraintLeft_toRightOf="@+id/tv_glucose_average"
                            tools:ignore="ContentDescription" />
                    </LinearLayout>

                    <TextView style="@style/trend_desc_TIR"
                        android:id="@+id/desc_high"
                        app:layout_constraintBottom_toTopOf="@id/ll_normal"
                        app:layout_constraintTop_toBottomOf="@id/ll_high"
                        app:layout_constraintStart_toStartOf="parent"/>

                    <LinearLayout
                        android:layout_marginTop="10dp"
                        app:layout_constraintBottom_toTopOf="@id/desc_normal"
                        app:layout_constraintTop_toBottomOf="@id/desc_high"
                        android:id="@+id/ll_normal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical">

                        <ImageView
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:background="@drawable/shape_trend_c4"
                            tools:ignore="ContentDescription" />

                        <TextView
                            style="@style/trend_text_TIR"
                            android:text="@string/normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:id="@+id/normal_percent"
                            android:text="0%" />

                    </LinearLayout>


                    <TextView style="@style/trend_desc_TIR"
                        android:id="@+id/desc_normal"
                        app:layout_constraintBottom_toTopOf="@id/ll_low"
                        app:layout_constraintTop_toBottomOf="@id/ll_normal"
                        app:layout_constraintStart_toStartOf="parent"/>

                    <LinearLayout
                        android:layout_marginTop="10dp"
                        app:layout_constraintBottom_toTopOf="@id/desc_low"
                        android:id="@+id/ll_low"
                        app:layout_constraintTop_toBottomOf="@id/desc_normal"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical">

                        <ImageView
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:background="@drawable/shape_trend_c1"
                            tools:ignore="ContentDescription" />

                        <TextView
                            style="@style/trend_text_TIR"
                            android:text="@string/hypo" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:id="@+id/low_percent"
                            android:text="0%" />

                        <ImageView
                            android:id="@+id/iv_low_trends"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_marginStart="6dp"
                            android:src="@drawable/ic_trends_up"
                            android:visibility="visible"
                            app:layout_constraintBottom_toTopOf="@+id/tv_unit"
                            app:layout_constraintLeft_toRightOf="@+id/tv_glucose_average"
                            tools:ignore="ContentDescription" />

                    </LinearLayout>


                    <TextView style="@style/trend_desc_TIR"
                        android:id="@+id/desc_low"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/ll_low"/>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                android:layout_marginStart="20dp"
                android:id="@+id/txt_title_agp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:drawableRight="@drawable/ic_question"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:text="@string/daily_trends"
                android:textSize="16sp" />

            <!--            APP-SRC-A-7-3-->
            <LinearLayout
                android:id="@+id/ll_trends"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:background="@drawable/shape_circle_light"
                android:orientation="vertical"
                android:paddingBottom="6dp">

                <com.microtechmd.cgms.widget.chart.TrendsChart
                    android:id="@+id/chart"
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="220dp" />

                <RelativeLayout
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:gravity="center">

                        <View
                            android:layout_width="20dp"
                            android:layout_height="1.5dp"
                            android:background="?attr/colorTrendChartMain" />

                        <TextView
                            style="@style/trend_text_AGP"
                            android:text="@string/trend_agp_1" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:gravity="center">

                        <View
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:background="?attr/colorTrendChart75" />

                        <TextView
                            style="@style/trend_text_AGP"
                            android:text="@string/trend_agp_2" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:gravity="center">

                        <View
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:background="?attr/colorTrendChart90" />

                        <TextView
                            style="@style/trend_text_AGP"
                            android:text="@string/trend_agp_3" />
                    </LinearLayout>

                </RelativeLayout>
            </LinearLayout>

            <TextView
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:textStyle="bold"
                android:id="@+id/txt_title_multi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="@string/muti_day_glucose"
                android:textSize="16sp" />

            <LinearLayout
                android:id="@+id/ll_multi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/shape_circle_light"
                android:orientation="vertical"
                android:paddingBottom="6dp">

                <com.microtechmd.cgms.fragment.trends.MultiChart
                    android:id="@+id/chart_multi"
                    android:layout_width="match_parent"
                    android:layout_height="220dp"
                    android:layout_marginTop="10dp" />

                <com.microtechmd.cgms.fragment.trends.ExpandableGrid
                    android:id="@+id/expandable_grid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <TextView
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:textStyle="bold"
                android:id="@+id/txt_title_LBGI"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:text="@string/trends_content6"
                android:textSize="16sp"
                app:drawableRightCompat="@drawable/ic_question" />

            <!--            APP-SRC-A-7-5-->
            <LinearLayout
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/shape_circle_light">

                <com.microtechmd.cgms.fragment.trends.LbgiCursorView
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    android:id="@+id/cursor_view"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="30dp"/>

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</LinearLayout>
